{
	"name": "shopeca/xml-generator",
	"description": "XML generator with Latte template support",
	"version": "1.0.5",
	"keywords": [
		"latte",
		"xml"
	],
	"license": [
		"MIT"
	],
	"authors": [
		{
			"name": "Tom Hnatovsky"
		},
		{
			"name": "Martin Knor"
		}
	],
	"require": {
		"php": ">=7",
		"nette/utils": "^3.0",
		"latte/latte": "^2.5",
		"nette/safe-stream": "^2.4",
		"nette/reflection": "^2.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^7",
		"phpstan/phpstan": "^0.11.19"
	},
	"autoload": {
		"psr-4": {
			"Shopeca\\XML\\": "src/"
		},
		"classmap": [
			"src/exceptions.php"
		]
	}
}
